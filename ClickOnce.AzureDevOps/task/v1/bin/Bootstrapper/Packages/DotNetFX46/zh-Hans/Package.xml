  </PackageFiles>
  <InstallChecks>
    <RegistryCheck Property="DotNet46FullLangPack_Release" Key="HKLM\Software\Microsoft\NET Framework Setup\NDP\v4\Full\2052" Value="Release"/>
    <RegistryCheck Property="DotNet46FullLangPack_OS_LCID" Key="HKLM\SYSTEM\CurrentControlSet\Control\MUI\UILanguages\zh-CN" Value="LCID"/>
