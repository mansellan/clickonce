  <Strings>
    <String Name="DisplayName">SQL Server 2008 R2 Express</String>
    <String Name="Culture">zh-Hans</String>
    <String Name="SqlExpr32Exe">http://go.microsoft.com/fwlink/?LinkID=230856&amp;clcid=0x804</String>
